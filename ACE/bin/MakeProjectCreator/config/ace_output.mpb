// -*- MPC -*-
project {
  specific(ghs) {
    // Green Hills MULTI IDE automatically prepends the directory path specified in __BUILD_DIR
    // to the path specified in libout. __BUILD_DIR is a macro in the top-project .gpj file (i.e.,
    // workspace file generated by MPC). E.g., if __BUILD_DIR is $(ACE_ROOT)/build, the directory
    // containing the libraries will be $(ACE_ROOT)/build/lib
    libout = lib
  } else {
    libout = $(ACE_ROOT)/lib
  }
}
